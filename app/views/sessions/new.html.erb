
<div class="sidenav">
   <div class="login-main-text">
      <h2>Market <br> PÃ¡gina de Login</h2>
      <p>Para ter acesso, por favor realize login.</p>
   </div>
</div>
<div class="main">
   <div class="col-md-6 col-sm-12">
      <div class="login-form">
         <%= form_for :session, url: entrar_path, method: :post do |f| %>
            <div class="form-group">
               <%= f.label :mat %>
               <%= f.text_field :mat, class: "form-control", placeholder: "Informe sua matricula", autofocus: "true" %>
            </div>
            <div class="form-group">
               <%= f.label :password %>
               <%= f.password_field :password, class: "form-control", placeholder: "Informe sua senha" %>
            </div>
            <%= f.submit "Entrar" , class: "btn btn-black" %>
         <% end -%>
      </div>
   </div>
</div>
